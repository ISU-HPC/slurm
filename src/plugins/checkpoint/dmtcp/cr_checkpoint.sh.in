#!/bin/sh
set -x

source /etc/profile

SLURM_CHECKPOINT_IMAGE_DIR=$1
SLURM_JOB_ID=$2
folder=$SLURM_CHECKPOINT_IMAGE_DIR/$SLURM_JOB_ID
mkdir -p $folder
cd $folder

checkpoint_info=$SLURM_CHECKPOINT_IMAGE_DIR"/checkpoint_info."$SLURM_JOB_ID
source $checkpoint_info

@DMTCP_HOME@/dmtcp_command  bc

exit $?

